{
  "name": "division",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "electron": "chcp 65001 && electron .",
    "electron-win": "electron-packager . division --win --out ./buildElectron --arch=x64 --asar --app-version=0.1.2 --electron-version=2.0.5",
    "electron-Win": "electron-packager . 'Hosts' --platform=win32 --out ./buildElectron --arch=x64 --asar --app-version=0.1.2 --electron-version=2.0.5",
    "electron-linux": "electron-packager . 'Hosts' --platform=linux --out ./buildElectron --arch=x64 --asar --app-version=0.1.2 --electron-version=2.0.5",
    "electron-darwin": "electron-packager . 'Hosts' --platform=darwin --out ./buildElectron --arch=x64 --asar --app-version=0.1.2 --electron-version=2.0.5",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "restart": "del .\\node_modules && npm install",
    "restart-yarn": "del .\\node_modules && yarn install",
    "rebuild": ".\\node_modules\\.bin\\electron-rebuild",
    "test": "node scripts/test.js --env=jsdom"
  },
  "babel": {
    "presets": [
      "react-app",
      "react",
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@material-ui/core": "^3.0.1",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^1.0.0-alpha.11",
    "autoprefixer": "^9.1.3",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^2.0.0",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mobx": "^5.1.0",
    "mobx-react": "^5.2.5",
    "mobx-react-devtools": "^6.0.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "rc-color-picker": "^1.2.6",
    "react": "^16.4.2",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.4.2",
    "react-popper": "^1.0.2",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.0.0",
    "resolve": "^1.8.1",
    "style-loader": "^0.23.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.1.1",
    "webpack": "3.12.0",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "electron": "^2.0.8",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.5.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
