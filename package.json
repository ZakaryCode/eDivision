{
  "name": "division",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": ".",
  "scripts": {
    "electron": "chcp 65001 && electron .",
    "electron-win": "electron-packager . division --win --out ./buildElectron --arch=x64 --asar --app-version=0.1.2 --electron-version=2.0.5",
    "electron-Win": "electron-packager . 'Hosts' --platform=win32 --out ./buildElectron --arch=x64 --asar --app-version=0.1.2 --electron-version=2.0.5",
    "electron-linux": "electron-packager . 'Hosts' --platform=linux --out ./buildElectron --arch=x64 --asar --app-version=0.1.2 --electron-version=2.0.5",
    "electron-darwin": "electron-packager . 'Hosts' --platform=darwin --out ./buildElectron --arch=x64 --asar --app-version=0.1.2 --electron-version=2.0.5",
    "start": "cross-env NODE_ENV=development react-app-rewired start",
    "build": "cross-env NODE_ENV=production react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "restart": "del .\\node_modules && npm install",
    "restart-yarn": "del .\\node_modules && yarn install",
    "rebuild": ".\\node_modules\\.bin\\electron-rebuild",
    "lint": "pretty-quick --pattern \"**/*.{ts,tsx}\"",
    "lint-all": "prettier --config ./.prettierrc --write \"**/*.{ts,tsx}\"",
    "tslint": "tslint --project tsconfig.json \"**/*.{ts,tsx}\"",
    "tslint-fix": "tslint --project tsconfig.json \"**/*.{ts,tsx}\" --fix",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "chalk": "^2.4.2",
    "dotenv": "^6.2.0",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "mobx-react-devtools": "^6.1.1",
    "rc-color-picker": "^1.2.6",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-loadable": "^5.5.0",
    "react-popper": "^1.3.3",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.4.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/react-loadable": "^5.5.0",
    "@types/react-router-dom": "^4.3.1",
    "cross-env": "^5.2.0",
    "customize-cra": "^0.2.12",
    "electron": "^4.0.7",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "prettier-tslint": "^0.4.2",
    "pretty-quick": "^1.10.0",
    "react-app-rewired": "^2.1.0",
    "react-scripts": "^2.1.8",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333",
    "typescript-tslint-plugin": "^0.3.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
